#to calculate hotel tariff
month=int(input("Month:\n"))
rent=float(input("Rent:\n"))
days=int(input("NO of days:\n"))
total=rent*days
if month == 1 or month == 2 or month == 3 or month == 7 or month == 8 or month == 9 or month == 10:
    tariff=total
    print(f"Hotel Tariff: Rs.{'%.2f'%tariff}")
elif month == month == 4 or month == 5 or month == 6 or month == 11 or month == 12:
    tariff=1.2*total
    print(f"Hotel Tariff: Rs.{'%.2f'%tariff}")
else:
    print("Invalid Input")
